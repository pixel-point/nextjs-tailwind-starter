{
  "name": "nextjs-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write .",
    "lint": "npm run lint:js & npm run lint:styles",
    "lint:fix": "npm run lint:js:fix & npm run lint:styles:fix",
    "lint:js": "eslint --ext .js,.jsx --ignore-path .gitignore .",
    "lint:js:fix": "eslint --fix --ext .js,.jsx --ignore-path .gitignore .",
    "lint:md": "markdownlint --ignore-path .gitignore .",
    "lint:md:fix": "markdownlint --fix --ignore-path .gitignore .",
    "prepare": "husky install"
  },
  "dependencies": {
    "clsx": "^1.1.1",
    "file-loader": "^6.2.0",
    "next": "^12.1.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tailwindcss": "^3.0.23",
    "tailwindcss-safe-area": "^0.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@svgr/webpack": "^6.2.1",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.9.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "postcss": "^8.4.6",
    "postcss-import": "^14.0.2",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.7",
    "svgo-loader": "^3.0.0",
    "url-loader": "^4.1.1"
  }
}
